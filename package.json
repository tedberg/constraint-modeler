    "@vue/cli-service": "^3.1.4",
    "@vue/eslint-config-standard": "^4.0.0",
    "@vue/test-utils": "^1.0.0-beta.25",
    "babel-core": "7.0.0-bridge.0",
    "babel-jest": "^23.6.0",
    "babel-plugin-jsx-v-model": "^2.0.3",
    "cross-env": "^5.2.0",
    "eslint-plugin-cypress": "^2.1.2",
